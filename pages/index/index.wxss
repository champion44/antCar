/**index.wxss**/

.mainContainer {
  background-color: rgb(239, 239, 239);
}

.swiper {
  height: 320rpx;
  width: 100%;
}

.swiper image {
  height: 100%;
  width: 100%;
}
.youStage{
    background-color: white;
width: 750rpx;
size: 10rpx;
}
.location {
  position: absolute;
  top: 1%;
  left: 31%;
  border-radius: 30px;
  background: rgba(135, 135, 135, 0.6);
  text-align: center;
  color: white;
  height: 50rpx;
  line-height: 50rpx;
  z-index: 99;
}

.locationImg {
  position: absolute;
  top: 1%;
  left: 20%;
  
}

.qaBtn {
  margin: 20rpx;
}

.icons {
  display: flex;
  background-color: white;
  margin-bottom: 20rpx;
}

.icon {
  width: 180rpx;
  margin: 7rpx;
}

.icon image {
  width: 80rpx;
  height: 80rpx;
  margin-left: 50rpx;
}

.icon p {
  display: flex;
  flex-direction: column;
  color: gray;
  text-align: center;
  font-size: 30rpx;
}


.scheduals {
  display: flex;
  background-color: white;
  margin-bottom: 20rpx;
}

.schedual {
  width: 150rpx;
  margin: 7rpx;
}

.schedual image {
  width: 50rpx;
  height: 50rpx;
  margin-left: 30rpx;
}

.schedual p {
  display: flex;
  flex-direction: column;
  color: gray;
  text-align: center;
  font-size: 25rpx;
  margin-left: 12rpx;
}
.articles {
  margin-bottom: 20rpx;
}
.topWrap{
  background-color: white;
}
.logo {
  width: 50rpx;
  height: 50rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

.articles text {
  position: absolute;
  margin-top: 20rpx;
  color: gray;
}

.moreArticles {
  margin-left: 75%;
}

.article {
  background-color: white;
  height: 200rpx;
  margin-bottom: 9rpx;
}
.wrapper{
  display: flex;
  flex-direction: row;
}
.column{
  display: flex;
  flex-direction: column;
}
.articlePic {
  width: 260rpx;
  height: 180rpx;
  margin: 10rpx 20rpx;
}
.noMore{
  height: 200rpx;
  background-color: #303030;
}
.noMore image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 15rpx;
  position: relative;
  top: 30%;
  left: 35%;
}
.noMore text{
   position: relative;
  top: 15%;
  left: 40%;
}
.stage{
  position: absolute;
  margin-top: 17rpx;
  font-size: 36rpx;
}

@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}


.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

